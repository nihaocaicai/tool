<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tool.api.dao.ErrorDao">
	<!--根据id查询用户信息-->
    <select id="findErrorById" parameterType="String" resultType="com.tool.api.entity.Error">
        select * from Error where user_id=#{id}
    </select>
</mapper>